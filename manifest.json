{
    "manifest_version": 2,    
    "name": "Codealyzer",
    "description": "A tool for rating code quality",
    "version": "0.1",
    "browser_action": {
        "default_title": "Codealyze!",
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "permissions": [
        "tabs",
        "identity",
        "identity.email"
    ]
}